package cn.edu.ccut.factory;

import cn.edu.ccut.service.IStudentService;
import cn.edu.ccut.service.impl.StudentServiceImpl;

public class ServiceFactory {
	private static IStudentService service = new StudentServiceImpl();
	
	public ServiceFactory() {}
	
	public static IStudentService createStudentService() {
		return service;
	}
}
