package cn.edu.ccut.service.impl;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.beans.factory.annotation.Qualifier;
import org.springframework.beans.factory.annotation.Required;

import cn.edu.ccut.dao.IStudentDAO;
import cn.edu.ccut.service.IStudentService;
import cn.edu.ccut.vo.Student;

public class StudentServiceImpl implements IStudentService {
	private IStudentDAO dao;
	
	public StudentServiceImpl() {
		// TODO Auto-generated constructor stub
	}
	
	public StudentServiceImpl(IStudentDAO dao) {
		this.dao = dao;
	}

	public IStudentDAO getDao() {
		return dao;
	}
	
	@Required
	@Autowired
	//@Qualifier("dao")
	public void setDao(IStudentDAO dao) {
		this.dao = dao;
	}

	@Override
	public boolean insert(Student vo){
		return dao.doCreate(vo);
	}
}
